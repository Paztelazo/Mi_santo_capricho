# Example environment variables for Backend
# Copy to a file named .env and fill with real values

# Postgres
DB_USER=your_db_user
DB_HOST=localhost
DB_NAME=your_db_name
DB_PASSWORD=your_db_password
DB_PORT=5432

# Server
PORT=4000

# Admin temporary credentials (used by the simple login)
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=1234

# JWT secret (for future use)
JWT_SECRET=replace_with_a_strong_secret
